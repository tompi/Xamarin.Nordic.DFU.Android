<?xml version="1.0"?>
<package>
  <metadata>
    <id>Xamarin.Nordic.DFU.Android</id>
    <version>2.3.0.26</version>
    <title>Nordic OTA DFU for Xamarin.Android</title>
    <authors>Laerdal Medical</authors>
    <owners>Laerdal Medical</owners>
    <icon>icon.png</icon>
    <readme>README.md</readme>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
Enable over the air(OTA) device firmware update(DFU) for Nordic devices using Android and Bluetooth Low Energy(BLE).

This nuget is just a binding library for this native Android library: https://github.com/NordicSemiconductor/Android-DFU-Library
    </description>

    <releaseNotes>Update to version 2.3.0 from the Nordic library.</releaseNotes>

    <copyright>Copyright Laerdal Medical 2023</copyright>

    <repository type="git" url="https://github.com/tompi/Xamarin.Nordic.DFU.Android"/>
    <license type="expression">MIT</license>
    <tags>Xamarin Nordic DFU Android</tags>

    <dependencies>
    <group targetFramework="MonoAndroid12.0">
        <dependency id="Xamarin.AndroidX.Annotation" version="1.5.0.2" />
        <dependency id="Xamarin.AndroidX.Core" version="1.9.0.2" />
        <dependency id="Xamarin.AndroidX.LocalBroadcastManager" version="1.1.0.4" />
    </group>
    </dependencies>

  </metadata>

  <files>
	<file src="Xamarin.Nordic.DFU.Android/bin/Release/Xamarin.Nordic.DFU.Android.dll" target="lib\MonoAndroid12.0" />
	<file src="Xamarin.Nordic.DFU.Android/bin/Release/Xamarin.Nordic.DFU.Android.pdb" target="lib\MonoAndroid12.0" />
   	<file src="icon.png"/>
   	<file src="README.md"/>
  </files>

</package>
